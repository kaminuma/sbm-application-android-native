# SBM Android Application - Local Configuration
# Copy this file to local.properties and fill in your actual values

# Android SDK path (update with your actual path)
sdk.dir=/Users/yourname/Library/Android/sdk

# ===========================================
# バックエンドAPI設定
# ===========================================

# バックエンドAPIのベースURL
# 本番環境: https://api.sbm-app.com/api/v1/
# 開発環境: http://localhost:8080/api/v1/
# LAN環境（実機テスト用）: http://192.168.1.100:8080/api/v1/
API_BASE_URL=https://api.sbm-app.com/api/v1/

# ===========================================
# Gemini API設定（直接呼び出し用）
# ===========================================

# Gemini APIキー - Google AI Studio (https://makersuite.google.com/app/apikey) で取得
GEMINI_API_KEY=your_gemini_api_key_here